{
  "expo": {
    "name": "Great Places",
    "slug": "great-places-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/places.png",
    "splash": {
      "image": "./assets/splash_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#171717"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package":"com.mnm.great_places",
      "versionCode":1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/places-adaptive.png",
        "backgroundColor": "#171717"
      },
      "permissions":[ "CAMERA", "ACCESS_FINE_LOCATION","WRITE_EXTERNAL_STORAGE"]

    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
